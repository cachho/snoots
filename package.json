{
  "name": "snoots",
  "version": "1.0.0-dev.14",
  "description": "A modern, fully-featured, strongly-typed reddit api.",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "tsc",
    "test": "jest",
    "test:cover": "jest --collect-coverage",
    "clean": "del-cli dist docs types",
    "lint": "npm run lint:check",
    "lint:check": "npm-run-all lint:check:*",
    "lint:check:eslint": "eslint \"**/*.ts\"",
    "lint:check:prettier": "prettier --check \"**/*\"",
    "lint:fix": "npm-run-all lint:fix:*",
    "lint:fix:eslint": "eslint --fix \"**/*.ts\"",
    "lint:fix:prettier": "prettier --write \"**/*\"",
    "gendoc": "typedoc src/index.ts",
    "prepack": "del-cli \"{dist,types}/**/__types__\"",
    "prepublishOnly": "npm run clean && npm run lint && npm run build"
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "got": "^11.8.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "del-cli": "^3.0.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "fast-check": "^2.14.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "nock": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "semantic-release": "^17.4.3",
    "ts-jest": "^27.1.3",
    "typedoc": "~0.20.36",
    "typedoc-plugin-rename-defaults": "^0.1.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">= 14"
  },
  "keywords": [
    "reddit",
    "snoo",
    "snoowrap"
  ],
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thislooksfun/snoots.git"
  },
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "bugs": {
    "url": "https://github.com/thislooksfun/snoots/issues"
  },
  "homepage": "https://github.com/thislooksfun/snoots#readme"
}
